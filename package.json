{
  "name": "@es-joy/estraverse",
  "description": "ECMAScript JS AST traversal functions",
  "homepage": "https://github.com/es-joy/estraverse",
  "bugs": "https://github.com/es-joy/estraverse/issues",
  "type": "module",
  "main": "./dist/estraverse.min.js",
  "exports": {
    "types": "./dist/estraverse.d.ts",
    "import": "./dist/estraverse.esm.min.js",
    "require": "./dist/estraverse.min.js"
  },
  "version": "6.0.0",
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "traversal"
  ],
  "author": {
    "name": "Yusuke Suzuki",
    "email": "utatane.tea@gmail.com",
    "web": "http://github.com/Constellation"
  },
  "contributors": [
    "Brett Zamir"
  ],
  "files": [
    "estraverse.js",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/es-joy/estraverse.git"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/register": "^7.24.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@types/estree": "^1.0.6",
    "acorn": "^8.12.1",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "eslint": "^9.11.1",
    "espree": "^10.2.0",
    "esprima": "^4.0.1",
    "globals": "^15.9.0",
    "mocha": "^10.7.3",
    "rollup": "^4.22.5",
    "typescript": "^5.6.2"
  },
  "license": "BSD-2-Clause",
  "c8": {
    "checkCoverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "test"
    ]
  },
  "scripts": {
    "tsc": "tsc",
    "build": "rollup -c && npm run tsc",
    "test": "npm run lint && npm run unit-test",
    "lint": "eslint .",
    "unit-test": "c8 mocha --require chai/register-expect.js --require @babel/register"
  }
}
